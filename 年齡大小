#include <iostream>
int min(int a,int b){
    int v_min =0;
    if (a < b){
        v_min = b;
    }else{
        v_min = a;
    }
    return v_min;
}

int main()
{
    int a,b;
    printf("piease input a:");
    scanf("%d",&a);
    printf("piease input b");
    scanf("%d",&b);
    printf("min = %d\n",min(a,b));
}
